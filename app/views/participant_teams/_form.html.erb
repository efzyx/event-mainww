<hr>
<%= form_for @participant, url: event_team_participant_teams_create_path, :html => {:class => "form-horizontal form-label-left"} do |f| %>
  <%= render "shared/errors", object: @participant %>
  <div class="form-group">
    <%= f.label :display_name, "Peserta :", class: "control-label col-md-3 col-sm-3 col-xs-12" %>
      <div class="col-md-6 col-sm-6 col-xs-12">
        <%= f.collection_select(:id, @participant_av, :id, :tele_username, {}, {:class => "form-control"}) %>
      </div>
  </div>
  <div class="ln_solid"></div>

  <%= f.hidden_field(:team_id, :value => @team.id) %>
  <%= f.hidden_field(:event_id, :value => @event.id) %>
  <div class="form-group">
    <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
      <%= link_to 'Kembali', @url, class: 'btn btn-danger' %>
      <%= f.submit "Simpan", class: "btn btn-primary pull-right" %>
    </div>
  </div>
<% end %>
